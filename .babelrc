{
  "presets": ["flow"],
  "plugins": [
    ["transform-runtime", { "polyfill": false, "regenerator": true }],
    ["transform-inline-environment-variables", {
      "include": [
        "NODE_ENV"
      ]
    }]
  ],
  "ignore": [
    "__tests__",
    "__mocks__"
  ],
  "env": {
    "es": {
      "presets": [
        ["env", { "modules": false, "debug": false }],
      ],
    },
    "cjs": {
      "presets": [
        ["env", { "modules": "commonjs", "debug": false }],
      ],
    },
    "test": {
      "plugins": ["dynamic-import-node"],
      "ignore": null
    }
  }
}
